{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "array-bracket-spacing": [2, "always"],
    "comma-dangle": 0,
    "consistent-return": 1,
    "curly": 0,
    "eqeqeq": [2, "smart"],
    "max-len": 1,
    "no-param-reassign": [2, { "props": false }],
    "no-return-assign": 0,
    "no-shadow": 1,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "prefer-template": 0,
    "semi": [2, "never"]
  }
}
